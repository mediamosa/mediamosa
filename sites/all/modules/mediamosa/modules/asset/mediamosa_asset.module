/**
 * Implements hook_cron().
 */
function mediamosa_asset_cron() {
  // We don't need garbage cleanup in simpletest.
  if (mediamosa::in_simpletest_sandbox()) {
    return;
  }

  try {
    $start = time();
    mediamosa_debug::log('mediamosa_asset_cron started.', array(), 'asset_cron');

    $run_last = variable_get('mediamosa_asset_cron_last', NULL);

    // may run?
    if (round(REQUEST_TIME - $run_last) >= mediamosa_settings::ASSET_GARBAGE_COLLECTOR_CRON_INTERVAL) {
      // For the next time...
      variable_set('mediamosa_asset_cron_last', REQUEST_TIME);

      // Do garbage cleanup.
      mediamosa_asset::do_garbage_cleanup();
    }

    // Log it.
    mediamosa_debug::log('mediamosa_asset_cron ended: @secs', array('@sec' => time() - $start), 'asset_cron');
  }
  catch (Exception $e) {
    mediamosa_watchdog::log('Catch exception during cron: mediamosa_asset_cron(): @msg', array('@msg' => $e->getMessage()));
  }
}

/**
 * Implements hook_menu_alter().
 */
function mediamosa_asset_menu_alter(&$items) {
  $items['admin/config/media/image-styles']['access callback'] = '_mediamosa_asset_admin_config_media_image_styles_access_callback';
}

function _mediamosa_asset_admin_config_media_image_styles_access_callback() {
  // Only the Mediamosa admin can acces that page.
  return (user_access('administer image styles') && user_access(mediamosa_permission::ADMIN_MEDIAMOSA));
}

/**
 * Implements hook_form_alter().
 */
function mediamosa_asset_form_alter(&$form, &$form_state, $form_id) {
  // Is this user MediaMosa administratior?
  $mediamosa_admin = user_access('administer image styles') && user_access(mediamosa_permission::ADMIN_MEDIAMOSA);

  if ($form_id == 'image_style_add_form') {
    // Image style to client application add form.

    $app_id = arg(5);

    if (!isset($app_id) || !is_numeric($app_id)) {
      // Does admin uses the image style page directly?
      if (!$mediamosa_admin) {
        drupal_access_denied();
        drupal_goto('');
      }
      return;
    }

    $app = mediamosa_app::get_by_appid($app_id);

    if (!$app || (!user_access(mediamosa_permission::ADMIN_MEDIAMOSA . '|app|' . $app_id) && !$mediamosa_admin)) {
      drupal_access_denied();
      drupal_goto('');
      return;
    }

    // Insert the style - app_id data.
    $form['#submit'][] = 'mediamosa_node_mediamosa_app_image_style_add_submit';

    $form['app_id'] = array(
      '#type' => 'value',
      '#value' => $app_id,
    );

    $form['back_to_client'] = array(
      '#markup' => l(t('Go back to the client application (app_id = @app_id) edit page', array('@app_id' => $app_id)), 'admin/mediamosa/config/app/' . $app[mediamosa_app_db::NID] . '/edit'),
    );
  }
  elseif ($form_id == 'image_style_form' || $form_id == 'image_style_delete_form') {
    // Image style to client application edit or delete form.

    $style = arg(5);

    if (!isset($style)) {
      // Does admin uses the image style page directly?
      if (!$mediamosa_admin) {
        drupal_access_denied();
        drupal_goto('');
      }
      return;
    }

    $still_style = mediamosa_asset_mediafile_still_style::get($style);
    $app_id = ($still_style[mediamosa_asset_mediafile_still_style_db::APP_ID] ? $still_style[mediamosa_asset_mediafile_still_style_db::APP_ID] : '');

    if (!$still_style || (!user_access(mediamosa_permission::ADMIN_MEDIAMOSA . '|app|' . $app_id) && !$mediamosa_admin)) {
      drupal_access_denied();
      drupal_goto('');
      return;
    }

    $app = mediamosa_app::get_by_appid($app_id);

    if ($form_id == 'image_style_form') {
      // If edit, then we update the style - app_id data and delete style images.
      $form['#submit'][] = 'mediamosa_node_mediamosa_app_image_style_edit_submit';
    }
    elseif ($form_id == 'image_style_delete_form') {
      foreach ($form['replacement']['#options'] as $key => $value) {
        if ($key) {
          if ($key == 'thumbnail' || $key == 'medium' || $key == 'large') {
            // We want to delete the default ones too.
            //continue;
          }
          $still_style = mediamosa_asset_mediafile_still_style::get($key);
          $app_id_replace = ($still_style[mediamosa_asset_mediafile_still_style_db::APP_ID] ? $still_style[mediamosa_asset_mediafile_still_style_db::APP_ID] : '');
          if (!$still_style || !user_access(mediamosa_permission::ADMIN_MEDIAMOSA . '|app|' . $app_id) || $app_id != $app_id_replace) {
            unset($form['replacement']['#options'][$key]);
          }
        }
      }
      // If delete, then we delete the style - app_id data and delete style images.
      $form['#submit'][] = 'mediamosa_node_mediamosa_app_image_style_delete_submit';
    }

    $form['style'] = array(
      '#type' => 'value',
      '#value' => $style,
    );
    $form['app_id'] = array(
      '#type' => 'value',
      '#value' => $app_id,
    );

    $form['back_to_client'] = array(
      '#markup' => l(t('Go back to the client application (app_id = @app_id) edit page', array('@app_id' => $app_id)), 'admin/mediamosa/config/app/' . $app[mediamosa_app_db::NID] . '/edit'),
    );
  }
}

